<%=@survey.title%>
<a href="/surveys/<%=@survey.id%>/edit">Edit Survey</a>

<%if @survey.questions%>
  <%@survey.questions.each do |question|%>
    <p><%=question.content%></p>
    <ul>
    <%question.choices.each do |choice|%>
          <li><%=choice.content%></li>
    <%end%>
  </ul>
  <%end%>
<%end%>




